{
  "name": "@smg-automotive/eslint-config",
  "version": "0.0.0-development",
  "description": "A boilerplate package setup",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./prettier": "./src/prettier.js",
    "./next": "./src/next.js",
    "./react": "./src/react.js",
    ".": "./src/index.js"
  },
  "files": [
    "src/**/*"
  ],
  "scripts": {
    "lint": "eslint --ext ts,js,tsx,jsx,json ./src --ignore-path ./src/__tests__/.eslintignore",
    "format": "npm run lint -- --fix",
    "test": "node src/__tests__/test.js"
  },
  "author": "SMG Automotive",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/smg-automotive/eslint-config-pkg.git"
  },
  "homepage": "https://github.com/smg-automotive/eslint-config-pkg#readme",
  "dependencies": {
    "@next/eslint-plugin-next": "12.1.6",
    "@rushstack/eslint-patch": "1.1.3",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.2.2",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-testing-library": "5.5.0"
  },
  "devDependencies": {
    "chalk": "4.1.2",
    "jest": "28.1.0",
    "prettier": "2.6.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.2"
  }
}
