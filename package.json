{
  "name": "@smg-automotive/eslint-config",
  "version": "0.0.0-development",
  "description": "A boilerplate package setup",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "version": "npm run build",
    "build": "rimraf pkg && tsc -p tsconfig.json --outDir dist",
    "lint": "eslint --ext ts,js,tsx,jsx,json .",
    "format": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "test": "npm run build && node dist/__tests__/test.js"
  },
  "author": "SMG Automotive",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/smg-automotive/eslint-config-pkg.git"
  },
  "homepage": "https://github.com/smg-automotive/eslint-config-pkg#readme",
  "dependencies": {
    "@next/eslint-plugin-next": "^12.1.6",
    "@rushstack/eslint-patch": "1.1.3",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/eslint-plugin-tslint": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "eslint": "8.14.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.1.5",
    "eslint-plugin-jsdoc": "39.2.9",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-testing-library": "5.3.1"
  },
  "devDependencies": {
    "@types/node": "17.0.31",
    "@types/prettier": "^2.6.0",
    "chalk": "^4.1.2",
    "jest": "28.0.3",
    "prettier": "^2.6.2",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.2",
    "ts-node": "^10.7.0",
    "typescript": "4.6.4"
  },
  "exports": {
    "./prettier": "./dist/prettier.js",
    "./next": "./dist/next.js",
    "./react": "./dist/react.js",
    ".": "./dist/index.js"
  },
  "files": [
    "dist/**/*"
  ]
}
